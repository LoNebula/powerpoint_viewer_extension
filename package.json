{
  "name": "powerpoint-viewer",
  "displayName": "PowerPoint Viewer",
  "description": "Display PowerPoint files (.pptx) as high-quality images with vertical scrolling within VS Code",
  "version": "1.3.0",
  "publisher": "LoNebula9",
  "author": {
    "name": "Shogo Miyawaki"
  },
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#D04423",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LoNebula/powerpoint_viewer_extension"
  },
  "bugs": {
    "url": "https://github.com/LoNebula/powerpoint_viewer_extension/issues"
  },
  "homepage": "https://github.com/LoNebula/powerpoint_viewer_extension#readme",
  "keywords": [
    "powerpoint",
    "pptx",
    "viewer",
    "preview",
    "presentation",
    "office",
    "slides"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [
    "onCustomEditor:powerpoint-viewer.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "powerpoint-viewer.preview",
        "displayName": "PowerPoint Preview",
        "selector": [
          {
            "filenamePattern": "*.pptx"
          }
        ],
        "priority": "option"
      }
    ],
    "commands": [
      {
        "command": "powerpoint-viewer.openPreview",
        "title": "PowerPoint: Open Preview"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "typescript": "^4.9.0",
    "@vscode/vsce": "^2.22.0"
  },
  "dependencies": {
  }
}
